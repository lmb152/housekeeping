/**index.wxss**/
.all{
  position:absolute;
  width:100%;
  height:100%;
  background:url(http://dev.iprotime.com/public/dd/xcx/kv_bg4.jpg) no-repeat;
  background-size:100% 100%;
  background:#012c35;
  over-flow:hidden;
}
.menu{
	position:fixed;
	top:20px;
	right:30px;
	z-index:99999;
	display:none;
}
.menu image{
	width:24px;
	height:16px;
}
.topimg{
	width:375px;
	height:358px;
	position:absolute;
	left:50%;
	margin-left:-187.5px;
}
.topimg image{
	width:100%;
	height:100%;
}
.starbutton{
	display:block;
	width:50%;
	position:absolute;
	bottom:20%;
	left:50%;
	margin-left:-25%;
	padding:20px 0;
	border:solid #63656a 1px;
	color:#ccc;
	border-radius:3px;
	font-size:14px;
	text-align:center;
}
.borderline{
	width:130px;
	height:130px;
	border-radius:50%;
	border:1px solid #dfcfa7;
	position:absolute;
	top:98.5px;
	left:50%;
	margin-left:-65px;
	opacity:0;
}
